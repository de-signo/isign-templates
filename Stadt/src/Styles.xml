<StyleList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://stollsoft.de/iSign/v2/Content/Forms/Styles">


  <style key="Stadt4.0" file="index.cshtml"
         interactive="true" target="chromium" preview="assets/img/preview.png">
    <names>
      <name culture="de">Stadt</name>
    </names>
    <fields>
      <field key="search">
        <names>
          <name>Suche-Optionen</name>
        </names>
         <content_class type="TextChoose">
          <text key="">Keine</text>
          <text key="abc">ABC Tastatur</text>
          <text key="qwertz">QWERTZ Tastatur</text>
        </content_class>
      </field>
      <field key="hooks">
        <names>
          <name>Liste-Optionen</name>
        </names>
         <content_class type="TextChoose">
          <text key="">Kein Schnellzugriff</text>
          <text key="yes">Mit Schnellzugriff</text>
        </content_class>
      </field>
      
      <field key="source">
        <names>
          <name>Datenquelle</name>
        </names>
        <content_class type="DataSource" />
      </field>
      <field key="cat">
        <names>
          <name>Kategorie</name>
        </names>
        <content_class bind="Column" parentField="source" type="Html" />
      </field>

      <field key="id">
        <names>
          <name>ID</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="term1">
        <names>
          <name>Begriff 1</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="term2">
        <names>
          <name>Begriff 2</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="house">
        <names>
          <name>Haus</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="level">
        <names>
          <name>Ebene</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="room">
        <names>
          <name>Raum</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="info">
        <names>
          <name>Info</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="phone">
        <names>
          <name>Telefon</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="email">
        <names>
          <name>E-Mail</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />
      </field>
      <field key="map">
        <names>
          <name>Lageplan</name>
        </names>
        <content_class bind="Column" parentField="source" type="PictureRef" />
      </field>
    </fields>
  </style>
</StyleList>